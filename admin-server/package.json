{
    "name": "admin-server",
    "version": "1.0.0",
    "description": "",
    "private": true,
    "egg": {
        "declarations": true
    },
    "dependencies": {
        "egg": "^2.15.1",
        "egg-cors": "^2.2.0",
        "egg-scripts": "^2.11.0",
        "egg-sequelize": "^5.1.0",
        "moment": "^2.24.0",
        "mysql2": "^1.6.5",
        "sequelize": "^5.9.0",
        "silly-datetime": "^0.1.2"
    },
    "devDependencies": {
        "autod": "^3.0.1",
        "autod-egg": "^1.1.0",
        "egg-bin": "^4.11.0",
        "egg-ci": "^1.11.0",
        "egg-mock": "^3.21.0",
        "eslint": "^5.13.0",
        "eslint-config-egg": "^7.1.0",
        "sequelize-cli": "^5.5.0"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "scripts": {
        "start": "egg-scripts start --daemon --title=egg-server-admin-server",
        "stop": "egg-scripts stop --title=egg-server-admin-server",
        "dev": "egg-bin dev",
        "debug": "egg-bin debug",
        "test": "npm run lint -- --fix && npm run test-local",
        "test-local": "egg-bin test",
        "cov": "egg-bin cov",
        "lint": "eslint .",
        "ci": "npm run lint && npm run cov",
        "autod": "autod",
        "sequelize": "sequelize --",
        "sequelize:reset": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all",
        "sequelize:migrate:do": "sequelize db:migrate",
        "sequelize:migrate:undo": "sequelize db:migrate:undo:all",
        "sequelize:seed:do": "sequelize db:seed:all",
        "sequelize:seed:undo": "sequelize db:seed:undo:all"
    },
    "ci": {
        "version": "10"
    },
    "repository": {
        "type": "git",
        "url": ""
    },
    "author": "Cillivian <h2ojeremy@hotmail.com>",
    "license": "MIT"
}
